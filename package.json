{
  "name": "mooks",
  "version": "1.7.0",
  "description": "",
  "main": "lib/index.js",
  "repository": "https://github.com/mcrowder65/mooks",
  "scripts": {
    "linter": "eslint src --ext .js",
    "jest": "jest --coverage --silent",
    "test": "yarn linter && yarn jest",
    "build": "babel src --out-dir lib --ignore \"src/**/*.spec.js\",\"src/**/*.test.js\"",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "preversion": "yarn test && yarn build",
    "postversion": "git push && git push --tags && yarn publish",
    "styleguide": "styleguidist server",
    "styleguide:build": "yarn && styleguidist build && rm -rf docs && mv styleguide docs"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "eslintConfig": {
    "extends": "mcrowder65"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/config.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "peerDependencies": {
    "@material-ui/core": "3.9.2",
    "jest-localstorage-mock": "2.4.0",
    "react": "16.8.4",
    "react-dom": "16.8.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-react": "7.0.0",
    "@material-ui/core": "3.9.2",
    "babel-jest": "24.0.0",
    "babel-loader": "8.0.5",
    "babel-plugin-import": "1.11.0",
    "babel-polyfill": "6.26.0",
    "coveralls": "3.0.2",
    "eslint-config-mcrowder65": "1.0.3",
    "jest": "24.4.0",
    "jest-dom": "3.0.1",
    "jest-localstorage-mock": "2.4.0",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-styleguidist": "9.0.4",
    "react-testing-library": "6.0.0",
    "webpack": "4.29.6"
  },
  "babelConfig": ".babelrc.js",
  "dependencies": {
    "store": "2.0.12"
  }
}
